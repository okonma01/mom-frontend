{
    "builds": [
        { "src": "server/app.py", "use": "@vercel/python" },
        { "src": "server/main.py", "use": "@vercel/python" },
        { "src": "server/data/**", "use": "@vercel/static" }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "server/app.py" },
        { "src": "/(.*)", "dest": "server/app.py" }
    ]
}